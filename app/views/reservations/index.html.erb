<div class="container">
 <h1> Reservations Overview </h1>
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h3> Here are all your reservations: </p>
      <ul class="list-group">
        <% @reservations.each do |reservation| %>
          <li class="list-group-item">
            <%= link_to reservation.name, reservation_path(reservation) %>
          </li>
        <% end %>
      </ul>
      <%= link_to "Add a new Reservation", new_reservation_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>