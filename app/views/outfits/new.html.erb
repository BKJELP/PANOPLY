<%= simple_form_for [ @user, @outfit ] do |f| %>
  <%= f.input :name %>
   <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :category %>
  <%= f.input :image %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create an outfit", class: "btn btn-primary" %>
<% end %>

<%=link_to  "Return", outfits_path %> %>